{
  "name": "@blaze-cardano/sdk",
  "version": "0.2.17",
  "description": "Blaze cardano sdk library",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "@blaze-cardano/eslint-config": "0.0.1",
    "@blaze-cardano/tsconfig": "0.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@blaze-cardano/core": "0.6.0",
    "@blaze-cardano/query": "0.3.4",
    "@blaze-cardano/tx": "0.10.1",
    "@blaze-cardano/uplc": "0.2.1",
    "@blaze-cardano/wallet": "0.3.6"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external rxjs",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external rxjs",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  }
}